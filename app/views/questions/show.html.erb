<p id="notice"><%= notice %></p>

<p>
  <strong>Title:</strong>
  <%= @question.title %>
</p>

<p>
  <strong>Body:</strong>
  <%= @question.body %>
</p>


<p>
  <strong>Answer:</strong>
  <%= @question.answer %>
</p>

<p>
  <strong>Contributor:</strong>
  <ul>
    <% @question.users.each do |user| %>
      <li><%= user.email %></li>
    <%end %>
  </ul>
</p>

<%= link_to 'Edit', edit_question_path(@question) %> |
<%= link_to 'Back', questions_path %>
